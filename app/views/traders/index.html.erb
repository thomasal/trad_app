<p id="notice"><%= notice %></p>

<h1>Listing Traders</h1>

<table>
  <thead>
    <tr>
      <th>Nom trader</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @traders.each do |trader| %>
      <tr>
        <td><%= trader.nom_trader %></td>
        <td><%= link_to 'Show', trader %></td>
        <td><%= link_to 'Edit', edit_trader_path(trader) %></td>
        <td><%= link_to 'Destroy', trader, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Trader', new_trader_path %>
